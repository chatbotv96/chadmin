                            @Html.DisplayFor(modelItem => item.Inputcontext)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Outputcontext)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Actionname)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Parameters)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ParamPrompt)
                        </td>

                        <td>
                            @Html.ActionLink("Edit", "Edit", "Intent", new { id = item.Id }, null)
