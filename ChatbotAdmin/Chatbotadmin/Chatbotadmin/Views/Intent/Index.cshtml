                <div class="col-md-10"> <h2>Intents</h2></div>
                <div class="col-md-2">  <button type="button" class="btn btn-primary btn-lg"><i class="icon-prev position-left"></i> Back to Dashboard</button></div>
            </div>
            <hr />
            <br />
            <table class="table table-bordered ">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.IntentName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Event)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Trainingphrase)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Botresponses)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Inputcontext)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Outputcontext)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Actionname)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Parameters)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ParamPrompt)
                    </th>

                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.IntentName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Event)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Trainingphrase)
